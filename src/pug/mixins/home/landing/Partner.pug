append variables 
  -
    var partners = [
      {
        img: '1.png',
      },
      {
        img: '2.png',
      },
      {
        img: '3.png',
      },
      {
        img: '4.png',
      },
      {
        img: '5.png',
      }
    ]

mixin PartnerItem(props)
  .col 
    .card.partner-card
      .card-body.text-center 
        img.img-fluid(src=`${CWD}assets/img/partner/${props.img}` alt="")  

mixin Partner 
  +Section.pt-0
    .row.row-cols-lg-5.row-cols-md-3.row-cols-2.align-items-center.justify-content-center 
      each item in partners 
        +PartnerItem(item)
      


